@model SpellsModel

<form action="/spell/add" method="post">

    <!--Iterates through each model object and displays its name-->
    @for (var i = 0; i < Model.ALL.Count; i++) {
        <a href="/spell/spell/@i">
            @Model.ALL[i].Name;
        </a><input type="checkbox" value="@Model.ALL[i].Name" name="spells" />
        <br />
    }

    <input type="submit" value="add" />
</form>
